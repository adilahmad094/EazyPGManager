                                app:layout_constraintTop_toBottomOf="@id/view3"
                                tools:layout_editor_absoluteX="12dp"
                                tools:layout_editor_absoluteY="82dp" />


                        </android.support.constraint.ConstraintLayout>


                    </android.support.v7.widget.CardView>

                </LinearLayout>

                <LinearLayout
