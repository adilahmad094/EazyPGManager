                app:layout_constraintTop_toBottomOf="@+id/subheadingTextView" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/productsansbold"
                android:text="Others"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/othersButton"
                app:layout_constraintStart_toStartOf="@+id/othersButton"
                app:layout_constraintTop_toBottomOf="@+id/othersButton" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/productsansbold"
                android:text="Rooms"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/roomButton"
                app:layout_constraintStart_toStartOf="@+id/roomButton"
                app:layout_constraintTop_toBottomOf="@+id/roomButton" />

            <TextView
                android:id="@+id/hyegineImprovementTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/productsansbold"
                android:text="Hyegine"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/hygieneButton"
                app:layout_constraintStart_toStartOf="@+id/hygieneButton"
                app:layout_constraintTop_toBottomOf="@+id/hygieneButton" />

            <TextView
